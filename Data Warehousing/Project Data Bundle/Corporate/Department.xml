<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <RECORD>
  <FIELD ID="1" xsi:type="CharTerm" TERMINATOR="\t" MAX_LENGTH="10" COLLATION="Latin1_General_BIN"/>
  <FIELD ID="2" xsi:type="CharTerm" TERMINATOR="\t" MAX_LENGTH="40" COLLATION="Latin1_General_BIN"/>
  <FIELD ID="3" xsi:type="CharTerm" TERMINATOR="\n" MAX_LENGTH="5" COLLATION="Latin1_General_BIN"/>
 </RECORD>
 <ROW>
  <COLUMN SOURCE="1" NAME="DEP_Code" xsi:type="SQLVARYCHAR"/>
  <COLUMN SOURCE="2" NAME="DEP_Name" xsi:type="SQLVARYCHAR"/>
  <COLUMN SOURCE="3" NAME="DEP_Class" xsi:type="SQLVARYCHAR"/>
 </ROW>
</BCPFORMAT>